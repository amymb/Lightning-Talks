<%=flash[:notice]%>
<%@talks.each do |talk|%>
  <%=link_to talk.speaker, talk%>
  <%=talk.topic%>
  <%=talk.date%>
  <%=talk.time%>
<%end%>
<%=link_to "New Talk", new_talk_path%>
<%=link_to "Sign In", sign_up_path%>

<%if current_user%>
  <p> Current User is <%=current_user.email%> </p>
  <%else%>
  <p> Nobody is here</p>
<%end%>
